{
  "name": "own-website",
  "version": "0.0.1",
  "private": true,
  "description": "My own website",
  "author": "What1s1ove",
  "scripts": {
    "start": "gulp start --development",
    "build": "gulp build",
    "lint": "npm run lint:editorconfig && npm run lint:html && npm run lint:css && npm run lint:js",
    "lint:html": "node-w3c-validator -i build/*.html -f lint -evH",
    "lint:css": "stylelint \"source/css/**/*.css\" --syntax css",
    "lint:js": "eslint --ext .js source/js",
    "lint:editorconfig": "editorconfig-checker"
  },
  "browserslist": [
    "last 2 version",
    "not dead"
  ],
  "nodeW3Cvalidator": {
    "suppressErrors": [
      "Attribute “loading” not allowed on element “img” at this point."
    ]
  },
  "devDependencies": {
    "@babel/core": "^7.9.6",
    "@babel/preset-env": "^7.9.6",
    "autoprefixer": "^9.7.6",
    "browser-sync": "^2.26.7",
    "css-mqpacker": "^7.0.0",
    "del": "^5.1.0",
    "editorconfig-checker": "^3.1.0",
    "eslint": "^7.0.0",
    "eslint-config-airbnb-base": "^14.1.0",
    "eslint-plugin-import": "^2.20.2",
    "gulp": "^4.0.2",
    "gulp-babel": "^8.0.0",
    "gulp-csso": "^4.0.1",
    "gulp-htmlmin": "^5.0.1",
    "gulp-if": "^3.0.0",
    "gulp-imagemin": "^7.1.0",
    "gulp-plumber": "^1.2.1",
    "gulp-postcss": "^8.0.0",
    "gulp-rename": "^2.0.0",
    "gulp-rollup": "^2.17.0",
    "gulp-sourcemaps": "^2.6.5",
    "gulp-svgstore": "^7.0.1",
    "gulp-terser": "^1.2.0",
    "gulp-webp": "^4.0.1",
    "imagemin": "^7.0.1",
    "imagemin-jpegtran": "^6.0.0",
    "imagemin-pngquant": "^8.0.0",
    "imagemin-svgo": "^7.1.0",
    "node-w3c-validator": "^1.6.1",
    "postcss-color-hex-alpha": "^6.0.0",
    "postcss-import": "^12.0.1",
    "prettier": "^2.0.5",
    "stylelint": "^13.3.3",
    "stylelint-config-prettier": "^8.0.1",
    "stylelint-prettier": "^1.1.2",
    "yargs": "^15.3.1"
  }
}
